{
  "name": "weldon/laravel-rabbit",
  "description": "Easy tool for working with RabbitMQ",
  "type": "library",
  "require": {
    "php": "^7.4|^8.0",
    "php-amqplib/php-amqplib": "^3.1",
    "illuminate/support": ">8"
  },
  "homepage": "https://github.com/weldon-tech/laravel-rabbit",
  "keywords": [
    "RabbitMQ",
    "Laravel RabbitMQ",
    "rabbit",
    "client"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\Rabbitmq\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Weldon",
      "email": "info.weldonuz@gmail.com",
      "homepage": "https://github.com/weldon-tech"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "App\\Rabbitmq\\Providers\\RabbitServiceProvider"
      ],
      "aliases": {
        "Rabbitmq": "App\\Rabbitmq\\Facades\\RabbitmqFacade"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
